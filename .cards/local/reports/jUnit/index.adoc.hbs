<?xml version="1.0" encoding="UTF-8"?>
<testsuites>
{{#each results }}
    <testsuite name="{{this.key}}">
{{#each results }}
        <testcase name="{{this.[base/fieldTypes/caseName]}}" classname="{{this.[base/fieldTypes/className]}} - {{this.[base/fieldTypes/cardTitle]}}" />
{{#if this.[base/fieldTypes/failureMessage]}}
            <failure message="{{this.[base/fieldTypes/failureMessage]}}">
            </failure>
{{/if}}
{{/each}}
    </testsuite>
{{/each}}
</testsuites>
