{{#each results}}
++++
  <div class="svg-item" style="width: 200px; height: 200px; font-size: 16px;">
  <svg width="100%" height="100%" viewBox="0 0 40 40" class="donut">
    <circle class="donut-hole" cx="20" cy="20" r="15.91549430918954" fill="#fff"></circle>
    <circle class="donut-ring" cx="20" cy="20" r="15.91549430918954" fill="transparent" stroke-width="3.5" style="stroke: #EBEBEB;"></circle>
    <circle class="donut-segment" cx="20" cy="20" r="15.91549430918954" fill="transparent" stroke-width="3.5" stroke-dasharray="{{../percentage}} {{inverse}}" stroke-dashoffset="25" style="stroke: #45d277;"></circle>
    <g class="donut-text" style="font-family: Arial, Helvetica, sans-serif; fill: #45d277;">
      <text y="50%" transform="translate(0, 2)">
        <tspan x="50%" text-anchor="middle" class="donut-percent"style="font-size: 0.5em; fill: #45d277; line-height: 1; transform: translateY(0.5em);">{{../percentage}}%</tspan>
      </text>
    </g>
  </svg>
</div>
++++
{{/each}}